PATH="/usr/local/bin:/usr/local/share:${PATH}"
PATH="/opt/nginx/sbin:${PATH}"                  # nginx
PATH="~/.rbenv/bin:~/.rbenv/shims:${PATH}"      # rbenv
PATH="${ZSH_CUSTOM}/common/scripts:${PATH}"     # User scripts
PATH="{$ZSH_CUSTOM}/${myhost}/scripts:${PATH}"

# Rbenv
eval "$(rbenv init -)"

