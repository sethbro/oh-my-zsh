## ALIASES 
# BroPro is a Mac, so some of these may be OS X specific.

alias ls="ls -C"
alias ll="ls -aClhF"


# ========== CLEARSTORY

alias gopostgres="postgres -D /usr/local/var/postgres -r /usr/local/var/postgres/server.log"
alias gocat="cd ${d_cs}/catalog-service/catalog-service && java -Ddw.http.port=6789 -jar ./target/catalog-service-0.2.14-SNAPSHOT.jar server local-test-config.yaml"
alias gostory="cd $d_cs/story-webapp && bundle exec rails server thin -e development"
alias gostoryaws="cd $d_cs/story-webapp && bundle exec rails server thin -e development_aws_catalog"

## Programs
alias mysql="sudo /usr/local/mysql/bin/mysql"

alias vht="cd $d_www/vht/viewer"

alias tidy="cd ~/Downloads && mv *.avi *.mov *.mp4 *.flv *.mpg *.mpeg *.wmv ~/dwhelper/ && cd ~"
alias add_music="cd ~/Downloads && mv *.mp3 ~/Music/iTunes/iTunes\ Media/Music/ && cd ~"


# Flash
export log_as="/Users/seth/Library/Preferences/Macromedia/Flash Player/Logs/flashlog.txt"
alias mmcfg='vim /Library/Application\ Support/Macromedia/mm.cfg'
alias wipe_as="wipe ${log_as}"

